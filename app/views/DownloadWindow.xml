<Alloy>
	<Collection src="Demo">
	<Window title="Download" class="container">
		<RightNavButton>
			<Button title="Logout" onClick="logout"/>
		</RightNavButton>
		<LeftNavButton>
			<Label id="username"></Label>
		</LeftNavButton>
		<TableView dataCollection="Demo" onClick="openDetail">
			<TableViewRow hasChild="true">
				<ImageView image="{Thumbnail}"/>
				<Label id="title" text="{Title}"/>
				<Label id="desc" text="{Description}"/>
			</TableViewRow>
		</TableView>
	</Window>
</Alloy>